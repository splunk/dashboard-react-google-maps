{
  "name": "@splunk/visualization-encoding",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.3.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build:ci": "yarn build && yarn docs",
    "clean": "yarn clean:webpack && yarn clean:test-reports",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "eslint src --ext \".js,.jsx\" -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "format:fix": "git ls-files | grep -E '\\.(es|jsx?|tsx?|css|md)$' | xargs prettier --write",
    "lint": "yarn run eslint",
    "lint:ci": "yarn run eslint:ci",
    "lint:ts": "eslint . --ext \".ts,.tsx\" --config ../../.tslintrc.js --no-eslintrc",
    "publish:ci": "cicd-publish-package",
    "test:unit": "jest src/.*",
    "test:unit:ci": "yarn run test:unit",
    "test:unit:watch": "jest --watch src/.*",
    "nearley": "nearleyc src/nearley/vizgrammar.ne -o src/nearley/generated/vizparser.js",
    "nearley:test": "nearley-test -i \"foo|bar(zap, 'dang', 23)|bloort()\" src/nearley/generated/vizparser.js"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.13.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/visualizations-shared": "^23.3.0",
    "@types/chroma-js": "^2.0.0",
    "@types/nearley": "^2.11.1",
    "chroma-js": "^2.0.0",
    "jexl": "^2.2.2",
    "lodash": "4.17.21",
    "nearley": "^2.19.1",
    "numbro": "2.3.5",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.0",
    "@splunk/cicd-tools": "^0.4.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/jsdoc-loader": "^3.0.0",
    "@splunk/visualization-build-tools": "^23.3.0",
    "@splunk/visualization-themes": "^23.3.0",
    "@splunk/visualizations-shared": "^23.3.0",
    "@splunk/webpack-configs": "^4.0.0",
    "@types/jest": "^26.0.0",
    "babel-jest": "^26.0.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.0.0",
    "raw-loader": "^4.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.2.1"
  }
}
