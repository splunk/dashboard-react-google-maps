{
  "name": "@splunk/visualization-themes",
  "description": "Dashboard themes for visualizations",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.4.0",
  "scripts": {
    "build": "babel src -d . --ignore \"**/*.test.js\"",
    "clean": "yarn clean:webpack && yarn clean:test-reports",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "babel src -d . --watch --ignore \"**/*.test.js\"",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:ci": "yarn run test:unit:ci && yarn run test:integration:ci",
    "test:integration": "echo integration test",
    "test:integration:ci": "echo integration test ci",
    "test:unit": "jest --passWithNoTests",
    "test:unit:ci": "yarn run test:unit -- --ci"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.0",
    "@splunk/themes": "^0.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.0",
    "@splunk/visualization-build-tools": "^23.4.0",
    "babel-jest": "^26.0.0",
    "jest": "^26.0.0"
  }
}
